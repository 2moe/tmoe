		cp -fv ${SOURCE_LIST} ${SOURCE_LIST}.bak 2>/dev/null
		if [[ ${TMOE_GITHUB} != true ]]; then
			modify_mint_source_list
		fi
		cp -fv ${SOURCE_LIST_02} ${SOURCE_LIST_02}.bak
		if [[ ${TMOE_GITHUB} != true ]]; then
			if [[ -e ${SOURCE_LIST_02} ]];then
				SOURCE_LIST=${SOURCE_LIST_02}
