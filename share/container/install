		git clone --depth=1 ${TMOE_PROOT_PROC_GIT} proot_proc || git clone --depth=1 ${TMOE_PROOT_PROC_GIT_02} proot_proc
	fi
	if [ ! -s "proot_proc/proc.tar.xz" ]; then
		mkdir -pv proot_proc
		aria2c --console-log-level=warn --no-conf -d proot_proc -o proc.tar.xz --allow-overwrite=true "${TMOE_PROOT_PROC_FILE_URL}"
	fi
	cd ${DEBIAN_CHROOT}
	if [ -e "${CONFIG_FOLDER}/proot_proc/proc.tar.xz" ]; then
		tar -Jxf ${CONFIG_FOLDER}/proot_proc/proc.tar.xz 2>/dev/null
	fi
else
	cd ${DEBIAN_CHROOT}
