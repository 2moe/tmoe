	void_linux_repository() {
	    LINUX_DISTRO='void'
		LOCALE_FILE="/etc/default/libc-locales"
		sed -i "s/^#.*${TMOE_LANG} UTF-8/${TMOE_LANG} UTF-8/" ${LOCALE_FILE} 2>/dev/null
