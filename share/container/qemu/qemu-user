		)
	}
	tmoe_qemu_user_manager_ja() {
		BETA_SYSTEM=$(
			whiptail --title "Across architectures" --menu "„Å©„ÅÆ„Ç¢„Éº„Ç≠„ÉÜ„ÇØ„ÉÅ„É£„Çí„Ç∑„Éü„É•„É¨„Éº„Éà„Åó„Åæ„Åô„ÅãÔºü" 0 50 0 \
				"0" "üåö Êàª„Çã" \
				"00" "qemu-user-static(Âü∫Êú¨ÁöÑ„Å™‰æùÂ≠òÈñ¢‰øÇ)" \
				"1" "i386/x86:Intel 8086„ÄÅ„Åä„Çà„Å≥„Åù„ÅÆÂæåÊñπ‰∫íÊèõÊÄß„ÇíÊåÅ„Å§„Éû„Ç§„ÇØ„É≠„Éó„É≠„Çª„ÉÉ„Çµ„ÅÆÂëΩ‰ª§„Çª„ÉÉ„Éà„Ç¢„Éº„Ç≠„ÉÜ„ÇØ„ÉÅ„É£„ÅÆÁ∑èÁß∞" \
				"2" "x64/amd64:„Åæ„Åü„ÅØx86-64„Å®„ÅØ„ÄÅx86„Ç¢„Éº„Ç≠„ÉÜ„ÇØ„ÉÅ„É£„Çí64„Éì„ÉÉ„Éà„Å´Êã°Âºµ„Åó„ÅüÂëΩ‰ª§„Çª„ÉÉ„Éà„Ç¢„Éº„Ç≠„ÉÜ„ÇØ„ÉÅ„É£" \
