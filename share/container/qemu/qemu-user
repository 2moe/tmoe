	cd ${CONFIG_FOLDER}
	NEW_TMOE_ARCH=''
	qemu_user_env
	RETURN_TO_WHERE='tmoe_qemu_user_manager'
	tmoe_qemu_user_manager_zh() {
		BETA_SYSTEM=$(
			whiptail --title "Ë∑®Êû∂ÊûÑËøêË°åÂÆπÂô®" --menu "ÊÇ®ÊÉ≥Ë¶Å(Ê®°Êãü)ËøêË°åÂì™‰∏™Êû∂ÊûÑÔºü\nWhich architecture do you want to simulate?" 0 50 0 \
				"0" "üåö Return to previous menu ËøîÂõû‰∏äÁ∫ßËèúÂçï" \
				"00" "qemu-user-staticÁÆ°ÁêÜ(Ë∑®Êû∂ÊûÑÊ®°ÊãüÊâÄÈúÄÁöÑÂü∫Á°Ä‰æùËµñ)" \
				"1" "x86/i386(Â∏∏ËßÅ‰∫é32‰ΩçcpuÁöÑÊóßÂºè‰º†Áªüpc)" \
				"2" "x64/amd64(2020Âπ¥ÊúÄ‰∏ªÊµÅÁöÑ64‰ΩçÊû∂ÊûÑ,Â∫îÁî®‰∫épcÂíåÊúçÂä°Âô®Ôºâ" \
				"3" "arm64/aarch64(2020Âπ¥ÁßªÂä®Âπ≥Âè∞‰∏ªÊµÅcpuÊû∂ÊûÑ)" \
				"4" "armhf/armv7(32‰ΩçarmÊû∂ÊûÑ,ÊîØÊåÅÁ°¨ÊµÆÁÇπËøêÁÆó)" \
				"5" "riscv64(ÂºÄÊ∫êÊû∂ÊûÑ,Á≤æÁÆÄÊåá‰ª§ÈõÜ)" \
				"6" "ppc64el(PowerPC,Â∫îÁî®‰∫éÈÄö‰ø°„ÄÅÂ∑•Êéß„ÄÅËà™Â§©ÂõΩÈò≤Á≠âÈ¢ÜÂüü)" \
				"7" "s390x(Â∏∏ËßÅ‰∫éIBMÂ§ßÂûãÊú∫)" \
				"8" "mipsel(Â∏∏ËßÅ‰∫éÂµåÂÖ•ÂºèËÆæÂ§á)" \
				"9" "mips64el(Â∏∏ËßÅ‰∫éÈæôËäØcpu)" \
				"10" "armel(ÊîØÊåÅËΩØÊµÆÁÇπËøêÁÆó,Â∏∏ËßÅ‰∫éÊóßËÆæÂ§áÔºâ" \
