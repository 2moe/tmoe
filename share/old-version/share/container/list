    CLOUD_ROOTFS=true
    DISTRO_NAME='devuan'
    #DISTRO_CODE='beowulf'
    #OLD_STABLE_VERSION=$(curl -L https://mirrors.bfsu.edu.cn/lxc-images/images/devuan/ | grep date | tail -n 2 | head -n 1 | cut -d '=' -f 4 | cut -d '"' -f 2)
    OLD_STABLE_VERSION=ascii
