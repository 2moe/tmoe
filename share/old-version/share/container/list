desktop_does_not_support() {
    printf "%s\n" "Sorry,tmoe-linuxå¼€å‘è€…æœªå¯¹å½“å‰æ¶æ„ï¼ˆ${ARCH_TYPE}ï¼‰é…ç½®${DESKTOP}çš„è‡ªåŠ¨æ„å»ºä»»åŠ¡ã€‚"
}
choose_mate_distro() {
    MENU="Which distribution do you want to install?"
    OPT_0="ğŸŒš Return to previous menu"
    OPT_1="ubuntu-dev(arm64, x64)"
    OPT_2="ğŸ¥ Debian-sid(arm64, x64)"
    OPT_3="ğŸ‘’ Fedora-rawhide(arm64, x64)"
    OPT_4="ğŸŒ‰ Arch-latest(arm64, x64)"
    OPT_5="ğŸ”ï¸ Alpine-edge(arm64, armhf, i386, x64)"
    #
    SELECTED_GNU_LINUX=$("${TUI_BIN:-whiptail}" --title "MATE DISTROS" --menu "${MENU}" 0 50 0 \
        "1" "$OPT_1" \
        "2" "$OPT_2" \
        "3" "$OPT_3" \
        "4" "$OPT_4" \
        "5" "$OPT_5" \
        "0" "$OPT_0" \
        3>&1 1>&2 2>&3)
    # "5" "ğŸŒ¿ Mint-tricia(amd64)" \
    case "${SELECTED_GNU_LINUX}" in
    0 | "") install_tmoe_weekly_build_edition ;;
    4)
        case ${ARCH_TYPE} in
        amd64 | arm64) set_arch_latest_env ;;
        *)
            desktop_does_not_support
            distro_does_not_support
            ;;
        esac
        ;;
    2)
        case ${ARCH_TYPE} in
        amd64 | arm64) set_debian_sid_env ;;
        *)
            desktop_does_not_support
            distro_does_not_support
            ;;
        esac
        ;;
    3)
        case ${ARCH_TYPE} in
        amd64 | arm64) set_fedora_rawhide_env ;;
        *)
            desktop_does_not_support
            distro_does_not_support
            ;;
        esac
        ;;
    5) set_alpine_edge_env ;;
    # 5)
    # 	case ${ARCH_TYPE} in
    # 	amd64) set_mint_tricia_env ;;
    # 	*)
    # 		desktop_does_not_support
    # 		distro_does_not_support
    # 		;;
    # 	esac
    # 	;;
    1)
        case ${ARCH_TYPE} in
        amd64 | arm64) set_ubuntu_dev_env ;;
        *)
            desktop_does_not_support
            distro_does_not_support
            ;;
        esac
        ;;
    esac
}
