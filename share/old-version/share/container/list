    DISTRO_NAME='void'
    TMOE_LINUX_CONTAINER_DISTRO='void'
    create_container_edition_txt
    go_to_proot_management
    DISTRO_NAME='voidlinux'
    DISTRO_CODE='default'
    DISTRO_CODE_02='current'
    linux_distro_common_model_03
}
