    DISTRO_NAME='ubuntu'
    DISTRO_CODE='focal'
    DEB_BOOT_STRAP=false
    RETURN_TO_WHERE='install_ubuntu_gnu_linux_distro'
    COMPATIBILITY_MODE=false
    TITLE="Which version do you want to install?"
    OPT_1="ğŸ 22.10 Kinetic Kudu"
    OPT_2="22.04(LTS) Jammy Jellyfish"
    # OPT_3="ğŸ’ 21.10(oldstable) Impish Indri"
    # OPT_3="ğŸ¦› 21.04 Hirsute Hippo"
    OPT_5="18.04(LTS) Bionic Beaver"
    OPT_6="ğŸ¿ï¸ 16.04(LTS) Xenial Xerus"
    install_ubuntu_gnu_linux_distro_zh() {
        MENU="2022è‡³2027å¹´çš„é•¿æœŸæ”¯æŒç‰ˆä¸ºjammy 22.04(2022å¹´4æœˆæ­£å¼å‘å¸ƒ)\nè®¾x=å½“å‰å¹´ä»½,è‹¥x>=2024,åˆ™è¯·æ‰‹åŠ¨è¾“å…¥ç‰ˆæœ¬ä»£å·"
        OPT_1="ğŸ 22.10 å‹•åŠ›ç¾šç¾Š Kinetic Kudu"
        OPT_2="22.04(LTS) æœé…±æ°´æ¯ Jammy Jellyfish"
        # OPT_3="ğŸ’ 21.10(oldstable) é ‘çš®å¤§ç‹çŒ´ Impish Indri"
        # OPT_3="ğŸ¦› 21.04 å¤šæ¯›æ²³é¦¬ Hirsute Hippo"
        OPT_4="ğŸ± 20.04(LTS)ç„¦é»é¦¬å³¶é•·å°¾ç‹¸è²“ Focal Fossa"
        OPT_5="18.04(LTS)ä»¿ç”Ÿæµ·ç‹¸ Bionic Beaver"
        OPT_6="ğŸ¿ï¸ 16.04(LTS)å¥½å®¢çš„éæ´²åœ°æ¾é¼  Xenial Xerus "
