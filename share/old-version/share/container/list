#!/usr/bin/env bash
# set -x
############################################
choose_which_gnu_linux_distro() {
    source ${TMOE_SHARE_DIR}/container/qemu/qemu-user
    RETURN_TO_WHERE='choose_which_gnu_linux_distro'
    RETURN_TO_MENU='choose_which_gnu_linux_distro'
    CLOUD_ROOTFS=false
    DEB_BOOT_STRAP=false
    TMOE_LINUX_CONTAINER_DISTRO=''
    check_rootfs_dir
    [[ ! -e ${CONFIG_FOLDER}/debootstrap_dir.txt ]] || rm -fv ${CONFIG_FOLDER}/debootstrap_dir.txt
    unset NODE01_ENABLED WEEKLY_BUILD_CONTAINER
    TITLE="CONTAINERS"
    OPT_10="â˜ï¸  Server: openEuler,Oracle,Amazon,Microsoft CBL-Mariner"
    choose_which_gnu_linux_distro_ja() {
        MENU="å®¹å™¨ã‚’ãŠé¸ã³ãã ã•ã„"
        OPT_0="ğŸŒš æˆ»ã‚‹"
        OPT_1="ğŸ­ æ¯é€±ã®GUIãƒ“ãƒ«ãƒ‰(kde,mate,xfce & lxqt)"
        OPT_2="ğŸ³ Rust,go,dotNET,swift,nodejs,jdk & docker"
        OPT_3="ğŸ¥ Debian ã¯ ã‚ªãƒšãƒ¬ãƒ¼ãƒ†ã‚£ãƒ³ã‚°ã‚·ã‚¹ãƒ†ãƒ ã®ãƒ•ãƒªãƒ¼ãªãƒ‡ã‚£ã‚¹ãƒˆãƒªãƒ“ãƒ¥ãƒ¼ã‚·ãƒ§ãƒ³ã§ã™"
        OPT_4="ğŸ› Ubuntu:é–‹ç™ºç›®æ¨™ã¯ã€Œèª°ã«ã§ã‚‚ä½¿ã„ã‚„ã™ã„æœ€æ–°ã‹ã¤å®‰å®šã—ãŸOSã€ã‚’æä¾›ã™ã‚‹ã“ã¨ã§ã‚ã‚‹"
        OPT_5="ğŸŒ‰ Arch:ã€Œã‚·ãƒ³ãƒ—ãƒªã‚·ãƒ†ã‚£ã€ã€ã€ŒãƒŸãƒ‹ãƒãƒªã‚ºãƒ ã€ã€ã€Œã‚¨ãƒ¬ã‚¬ãƒ³ã‚¹ã€ã•ã€ã‚³ãƒ¼ãƒ‰ã®æ­£ã—ã•ã«ç„¦ç‚¹ã‚’å½“ã¦ã¦é–‹ç™ºã•ã‚Œã¦ã„ã‚‹"
        OPT_6="ğŸ± Alpine,mint,rocky & puppy"
        OPT_7="ğŸ‰ Kali rolling:ãƒšãƒãƒˆãƒ¬ãƒ¼ã‚·ãƒ§ãƒ³ãƒ†ã‚¹ãƒˆã‚’ä¸»ãªç›®çš„ã¨ã—ã¦ã„ã‚‹"
        # OPT_8="ğŸŠ Armbian:ARMé–‹ç™ºãƒœãƒ¼ãƒ‰ç”¨ã®DebianãŠã‚ˆã³Ubuntuãƒ™ãƒ¼ã‚¹ã®ã‚³ãƒ³ãƒ”ãƒ¥ãƒ¼ã‚¿ãƒ¼ã‚ªãƒšãƒ¬ãƒ¼ãƒ†ã‚£ãƒ³ã‚°ã‚·ã‚¹ãƒ†ãƒ ã§ã™"
        OPT_9="ğŸ‘’ Fedora:ãƒ¬ãƒƒãƒ‰ãƒãƒƒãƒˆãŒæ”¯æ´ã™ã‚‹ã‚³ãƒŸãƒ¥ãƒ‹ãƒ†ã‚£ãƒ¼ã€ŒFedora Projectã€ã«ã‚ˆã£ã¦é–‹ç™ºã•ã‚Œã¦ã„ã‚‹RPMç³»Linux distro"
        OPT_11="ğŸ¦ Chrootå°‚å±ã™ã‚‹:openSUSE"
        OPT_12="ğŸ“ ä½“é¨“ç‰ˆ:RaspiOS,Gentoo"
    }
    choose_which_gnu_linux_distro_zh() {
        MENU="æ‚¨æƒ³è¦å®‰è£…å“ªä¸ªå®¹å™¨?"
        OPT_0="ğŸŒš Return to previous menu è¿”å›ä¸Šçº§èœå•"
        OPT_1="ğŸ­ æ¯å‘¨æ„å»º:kde,xfce,mate & lxqt"
        OPT_2="ğŸ³ Rust,go,dotNET,swift,nodejs,jdk & docker"
        OPT_3="ğŸ¥ Debian:è‡´åŠ›äºè‡ªç”±"
        OPT_4="ğŸ› Ubuntu:æˆ‘çš„å­˜åœ¨æ˜¯å› ç‚ºå¤§å®¶çš„å­˜åœ¨"
        OPT_5="ğŸŒ‰ Arch:ç³»ç»Ÿè®¾è®¡ä»¥KISSä¸ºæ€»ä½“æŒ‡å¯¼åŸåˆ™"
        OPT_6="ğŸ± Alpine,mint,rocky,solus & puppy"
        OPT_7="ğŸ‰ Kali rolling:è®¾è®¡ç”¨äºæ•°å­—å–è¯å’Œæ¸—é€æµ‹è¯•"
        # OPT_8="ğŸŠ Armbian:é¦™è•‰æ´¾,é¦™æ©™æ´¾(é¢„è£…xfce/cinnamon/budgie)"
        OPT_9="ğŸ‘’ Fedora:çº¢å¸½ç¤¾åŒºç‰ˆ,æ–°æŠ€æœ¯è¯•éªŒåœº"
        OPT_10="â˜ï¸  æœåŠ¡å™¨ç³»ç»Ÿ:åä¸ºæ¬§æ‹‰(openEuler),å¾®è½¯CBL-Mariner,ç”²éª¨æ–‡,äºšé©¬é€Š"
        OPT_11="ğŸ¦ Chrootä¸“å±:openSUSE, deepin"
        OPT_12="ğŸ“ ä½“éªŒç‰ˆ:archcraft,æ ‘è“æ´¾,gentoo"
    }
    choose_which_gnu_linux_distro_en() {
        MENU="Which container do you want to install?"
        OPT_0="ğŸŒš Return to previous menu"
        OPT_1="ğŸ­ Weekly builds(xfce,kde,mate & lxqt)"
        OPT_2="ğŸ³ Rust,go,dotNET,swift,nodejs,jdk & docker"
        OPT_3="ğŸ¥ Debian is a distro composed of free and open-source software"
        OPT_4="ğŸ› Ubuntu is the modern, open source OS."
        OPT_5="ğŸŒ‰ Arch:simplicity,modernity,pragmatism,user centrality and versatility"
        OPT_6="ğŸ± Alpine,mint,rocky & puppy"
        OPT_7="ğŸ‰ Kali:designed for digital forensics and penetration testing"
        # OPT_8="ğŸŠ Armbian:ARM development boards OS(gnome,cinnamon,budgie)"
        OPT_9="ğŸ‘’ Fedora:developed by the community-supported Fedora Project which is sponsored primarily by Red Hat"
        OPT_11="ğŸ¦ Chroot exclusive:openSUSE,deepin"
        OPT_12="ğŸ“ Experimental:RaspiOS,Gentoo,Archcraft"
    }
    choose_which_gnu_linux_distro_menu() {
        SELECTED_GNU_LINUX=$("${TUI_BIN:-whiptail}" --title "${TITLE}" --menu "${MENU}" 0 50 0 \
            "1" "$OPT_1" \
            "2" "$OPT_2" \
            "3" "$OPT_3" \
            "4" "$OPT_4" \
            "5" "$OPT_5" \
            "6" "$OPT_6" \
            "7" "$OPT_7" \
            "8" "$OPT_9" \
            "9" "$OPT_10" \
            "10" "$OPT_11" \
            "11" "$OPT_12" \
            "0" "$OPT_0" \
            3>&1 1>&2 2>&3)
    }
    ##############################
    case ${TMOE_MENU_LANG} in
    zh_*UTF-8) choose_which_gnu_linux_distro_zh ;;
    ja_JP.UTF-8) choose_which_gnu_linux_distro_ja ;;
    *) choose_which_gnu_linux_distro_en ;;
    esac
    choose_which_gnu_linux_distro_menu

    case "${SELECTED_GNU_LINUX}" in
    0 | "") proot_container_menu ;;
    1) install_tmoe_weekly_build_edition ;;
    2) install_tmoe_moonthly_build_edition ;;
    3) install_debian_gnu_linux_distro ;;
    4) install_ubuntu_gnu_linux_distro ;;
    5)
        DISTRO_NAME='arch'
        TMOE_LINUX_CONTAINER_DISTRO='arch'
        create_container_edition_txt
        go_to_proot_management
        ;;
    6) install_beta_containers ;;
    7)
        DISTRO_NAME='kali'
        DISTRO_CODE='rolling'
        TMOE_LINUX_CONTAINER_DISTRO='kali-rolling'
        create_container_edition_txt
        go_to_proot_management
        ;;
    # 8) install_armbian_linux_distro ;;
    8) install_fedora_gnu_linux_distro ;;
    9) cloud_server_distro_menu ;;
    10) install_chroot_exclusive_containers ;;
    11) install_alpha_containers ;;
    esac
    ####################
    exit 0
}
##############################
set_service_container_env_01() {
    DISTRO_CODE="${DESKTOP}"
    DISTRO_NAME_02="${DISTRO_NAME}"
    NODE01_ENABLED=false
    TMOE_LINUX_CONTAINER_DISTRO="${DISTRO_NAME}"
    create_container_edition_txt
    create_container_edition_txt_02
    go_to_proot_management
}
set_service_container_env_02() {
    DISTRO_CODE="${DESKTOP}"
    DISTRO_NAME_02="${DISTRO_NAME}"
    NODE01_ENABLED=false
    TMOE_LINUX_CONTAINER_DISTRO="${DISTRO_NAME}-${DESKTOP}"
    create_container_edition_txt
    create_container_edition_txt_02
    go_to_proot_management
}
set_service_container_env() {
    case ${DESKTOP} in
    alpine | dde) set_service_container_env_02 ;;
    latest | base | *) set_service_container_env_01 ;;
    esac
}
#####################
choose_latest_or_alpine() {
    if ("${TUI_BIN:-whiptail}" --title "TAGS" --yes-button "latest" --no-button "alpine" --yesno "In general, alpine is more streamlined" 8 50); then
        DESKTOP="latest"
    else
        DESKTOP="alpine"
    fi
}
choose_dde_or_nogui() {
    if ("${TUI_BIN:-whiptail}" --title "TAGS" --yes-button "dde" --no-button "nogui" --yesno "dde(deepin desktop environment).\nIn general, nogui is more streamlined." 8 50); then
        DESKTOP="dde"
    else
        DESKTOP="base"
    fi
}
cloud_server_distro_menu() {
    local MENU="Which distribution do you want to install?"
    local TITLE="CLOUD SERVER DISTROS"
    local OPT_0="ğŸŒš Return to previous menu"
    case ${TMOE_MENU_LANG} in
    zh_*UTF-8)
        local OPT_1="openEuler(arm64,x64,åä¸ºæ¬§æ‹‰ç¤¾åŒºç‰ˆ)"
        local OPT_2="Microsoft CBL-Mariner(ç”¨äºå¾®è½¯çš„äº‘åŸºç¡€è®¾æ–½å’Œè¾¹ç¼˜äº§å“)"
        local OPT_3="Oracleç”²éª¨æ–‡(arm64, x64)"
        local OPT_4="Amazonäºšé©¬é€Š(arm64, x64)"
        ;;
    *)
        local OPT_1="openEuler(arm64, x64)"
        local OPT_2="Microsoft CBL-Mariner(for Azure 1P services and edge appliances)"
        local OPT_3="Oracle(arm64, x64)"
        local OPT_4="Amazon(arm64, x64)"
        ;;
    esac
    DESKTOP="base"
    SELECTED_GNU_LINUX=$("${TUI_BIN:-whiptail}" --title "$TITLE" --menu "$MENU" 0 50 0 \
        "1" "$OPT_1" \
        "2" "$OPT_2" \
        "3" "$OPT_3" \
        "4" "$OPT_4" \
        "0" "$OPT_0" \
        3>&1 1>&2 2>&3)
    #"99" "Microsoft CBL-Mariner(x64,ä¸ºazureä¼˜åŒ–,ç”¨äºå¾®è½¯çš„äº‘åŸºç¡€è®¾æ–½å’Œè¾¹ç¼˜äº§å“)" \
    case ${ARCH_TYPE} in
    arm64 | amd64) WEEKLY_BUILD_CONTAINER=true ;;
    *) distro_does_not_support ;;
    esac
    case "${SELECTED_GNU_LINUX}" in
    0 | "") choose_which_gnu_linux_distro ;;
    1)
        DISTRO_NAME="euler"
        choose_dde_or_nogui
        ;;
    2) DISTRO_NAME="cblmariner" ;;
    3) DISTRO_NAME="oracle" ;;
    4) DISTRO_NAME="amazon" ;;
    esac
    set_service_container_env
    press_enter_to_return
    install_tmoe_moonthly_build_edition
}
install_tmoe_moonthly_build_edition() {
    choose_service_en() {
        INSTALLDESKTOP=$("${TUI_BIN:-whiptail}" --title "CONTAINERS" --menu \
            "Except docker, tmoe/azure server will automatically build and release the latest container every 2 weeks" 0 0 0 \
            "1" "ğŸ¦€ Rust(Blazingly fast and memory-efficient)" \
            "2" "ğŸ¹ Go (Makes it easy to build simple&reliable app)" \
            "3" "Visual Studio Code web server" \
            "4" "Microsoft .NET SDK(amd64, arm64)" \
            "5" "Swift(Mainly used for Apple iOS and MacOS app development)" \
            "6" "OpenJDK(An open-source Java Platform)" \
            "7" "ğŸ³ Docker(Run docker with qemu)" \
            "8" "Node.js(A back-end JavaScript runtime environment)" \
            "9" "Nginx(A high-performance HTTP server)" \
            "10" "PHP(A general-purpose scripting language)" \
            "11" "Ruby(An interpreted,general-purpose lang)" \
            "0" "ğŸŒš back =ï¿£Ï‰ï¿£=" \
            3>&1 1>&2 2>&3)
    }
    # "11" "MongoDB(Classified as a NoSQL database program)" \
    case ${TMOE_MENU_LANG} in
    zh_*UTF-8) choose_service_en ;;
    *) choose_service_en ;;
    esac
    case "${INSTALLDESKTOP}" in
    0 | "") choose_which_gnu_linux_distro ;;
    1)
        DISTRO_NAME="rust"
        choose_latest_or_alpine
        ;;
    2)
        DISTRO_NAME="go"
        choose_latest_or_alpine
        ;;
    3)
        DISTRO_NAME="code"
        DESKTOP="latest"
        ;;
    4)
        DISTRO_NAME="dotnet"
        DESKTOP="latest"
        ;;
    5)
        DISTRO_NAME="swift"
        DESKTOP="latest"
        ;;
    6)
        DISTRO_NAME="jdk"
        DESKTOP="latest"
        ;;
    7)
        DISTRO_NAME="docker"
        DESKTOP="qemu"
        ;;
    8)
        DISTRO_NAME="node"
        choose_latest_or_alpine
        ;;
    9)
        DISTRO_NAME="nginx"
        choose_latest_or_alpine
        ;;
    10)
        DISTRO_NAME="php"
        choose_latest_or_alpine
        ;;
    # 11)
    #     DISTRO_NAME="mongo"
    #     DESKTOP="latest"
    #     ;;
    11)
        DISTRO_NAME="ruby"
        choose_latest_or_alpine
        ;;
    esac

    case "${DISTRO_NAME}" in
    rust)
        case ${ARCH_TYPE} in
        arm64 | armhf | amd64 | riscv64 | ppc64el | s390x | mips64el) WEEKLY_BUILD_CONTAINER=true ;;
        *) distro_does_not_support ;;
        esac
        ;;
    dotnet | swift)
        case ${ARCH_TYPE} in
        arm64 | amd64) WEEKLY_BUILD_CONTAINER=true ;;
        *) distro_does_not_support ;;
        esac
        ;;
    *)
        case ${ARCH_TYPE} in
        arm64 | armhf | amd64) WEEKLY_BUILD_CONTAINER=true ;;
        *)
            printf "%s\n" "Sorry,this feature only supports arm64, amd64 & armhf"
            distro_does_not_support
            ;;
        esac
        ;;
    esac

    set_service_container_env
    press_enter_to_return
    install_tmoe_moonthly_build_edition
    : <<\EOF
	printf "%s\n" "${YELLOW}PREVIEW:${RESET}"
	printf "%s\n" "This category of containers will not automatically configure unnecessary steps."
	printf "%s\n" "A service or an environment is a container."
	printf "%s\n" "For example, you can type ${GREEN}tmoe p go${RESET} to start the ${BLUE}proot golang latest ${ARCH_TYPE}${RESET} container,if you are using tmoe-zsh ,just type ${GREEN}t p go${RESET}"
	printf "%s\n" "You can ${YELLOW}creat a new container${RESET} in the menu, just type the container name.If you type ${GREEN}1.16${RESET},then it will creat a new golang container.You can type ${GREEN}tmoe p go 1.16${RESET} to start it."

	printf "%s\n" "${RED}Sorry,${GREEN}this feature${YELLOW} is still ${BLUE}in alpha${RESET}"
	press_enter_to_return
	choose_which_gnu_linux_distro
EOF
}
##################################
install_tmoe_weekly_build_edition() {
    case ${ARCH_TYPE} in
    amd64 | i386 | arm64 | armhf) WEEKLY_BUILD_CONTAINER=true ;;
    *) distro_does_not_support ;;
    esac
    # TMOE/Azure Server will automatically build and release the latest DE container every week/month
    choose_de_en() {
        # MENU="Sorry, this feature will not be updated for a long time to come."
        MENU="TMOE/Azure Server will automatically build and release the latest DE container every week/month"
        INSTALLDESKTOP=$("${TUI_BIN:-whiptail}" --title "Desktop environment" --menu \
            "${MENU}" 0 0 0 \
            "1" "ğŸ­ xfce(elegant, high compatibility)" \
            "2" "ğŸ¦– kde plasma5(beautiful and modern)" \
            "3" "ğŸŒ¿ mate(the continuation of GNOME 2)" \
            "4" "ğŸ¦ lxqt" \
            "5" "ğŸ•Šï¸ lxde(lightweight)" \
            "6" "zsh(nogui)" \
            "7" "Weekly schedule" \
            "0" "ğŸŒš back =ï¿£Ï‰ï¿£=" \
            3>&1 1>&2 2>&3)
    }
    choose_de_zh() {
        # MENU="æŠ±æ­‰ï¼Œç”±äºæŸäº›åŸå› , æ­¤åŠŸèƒ½å°†åœ¨æ¥ä¸‹æ¥å¾ˆé•¿ä¸€æ®µæ—¶é—´åœæ›´"
        MENU="tmoe/azureæœåŠ¡å™¨æ¯å‘¨éƒ½ä¼šè‡ªåŠ¨æ„å»ºå¹¶å‘å¸ƒæœ€æ–°ç‰ˆ,å°‘éƒ¨åˆ†å‘è¡Œç‰ˆä¸ºæ¯æœˆæ›´æ–°"
        INSTALLDESKTOP=$("${TUI_BIN:-whiptail}" --title "Desktop environment" --menu \
            "${MENU}" 0 0 0 \
            "1" "ğŸ­ xfce(elegant, high compatibility)" \
            "2" "ğŸ¦– kde plasma5(é£æ ¼åä¸½çš„æ¡Œé¢ç¯å¢ƒ)" \
            "3" "ğŸŒ¿ mate(the continuation of GNOME 2)" \
            "4" "ğŸ¦ lxqt(arm64, x64)" \
            "5" "ğŸ•Šï¸ lxde(è½»é‡çº§, armhf, i386)" \
            "6" "zsh(nogui)" \
            "7" "schedule è‡ªåŠ¨æ„å»ºè®¡åˆ’æ—¥ç¨‹è¡¨" \
            "0" "ğŸŒš back =ï¿£Ï‰ï¿£=" \
            3>&1 1>&2 2>&3)
    }
    # "3" "ğŸŸ cutefish å¯çˆ±é±¼(arm64, x64, armhf)" \
    # "4" "dde æ·±åº¦æ¡Œé¢ç¯å¢ƒ(arm64, x64)" \
    # "5" "ukui ä¼˜éº’éºŸui(arm64, x64, armhf)" \
    #"7" "Request specific version" \"7" "request è¯·æ±‚æ›´æ–°/å¢åŠ ç‰ˆæœ¬" \
    case ${TMOE_MENU_LANG} in
    zh_*UTF-8) choose_de_zh ;;
    *) choose_de_en ;;
    esac
    case "${INSTALLDESKTOP}" in
    0 | "") choose_which_gnu_linux_distro ;;
    1)
        DESKTOP="xfce"
        choose_xfce_distro
        ;;
    2)
        DESKTOP="kde"
        choose_kde_distro
        ;;
    # 3)
    #     DESKTOP="cutefish"
    #     choose_cutefish_distro
    #     ;;
    # 4)
    # 	DESKTOP="dde"
    # 	choose_dde_distro
    # 	;;
    # 5)
    # 	DESKTOP="ukui"
    # 	choose_ukui_distro
    # 	;;
    3)
        DESKTOP="mate"
        choose_mate_distro
        ;;
    4)
        DESKTOP="lxqt"
        choose_lxqt_distro
        ;;
    5)
        DESKTOP="lxde"
        choose_lxde_distro
        ;;
    6)
        #printf "%s\n" "This feature may be available on 2020-02-17."
        DESKTOP="zsh"
        choose_zsh_distro
        ;;
    7) view_the_schedule ;;
        # 999) request_weekly_build_version ;;
    esac
    press_enter_to_return
    install_tmoe_weekly_build_edition
}
set_debian_sid_env() {
    DISTRO_NAME='debian'
    DISTRO_CODE="sid-${DESKTOP}"
    DISTRO_NAME_02=debian-sid
    NODE01_ENABLED=false
    #TMOE_LINUX_CONTAINER_DISTRO="${DISTRO_NAME}-${DISTRO_CODE}"
    TMOE_LINUX_CONTAINER_DISTRO="${DISTRO_NAME}-${DESKTOP}"
    create_container_edition_txt
    create_container_edition_txt_02
    go_to_proot_management
}
