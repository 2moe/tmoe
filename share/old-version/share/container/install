	add_arch_for_edu_repo() {
	    if ! grep -q 'arch4edu' /etc/pacman.conf; then
	        cat >>/etc/pacman.conf <<-'Endofpacman'
						[arch4edu]
						Server = https://mirrors.bfsu.edu.cn/arch4edu/$arch
						Server = https://mirrors.tuna.tsinghua.edu.cn/arch4edu/$arch
						Server = https://mirror.autisten.club/arch4edu/$arch
						Server = https://arch4edu.keybase.pub/$arch
						Server = https://mirror.lesviallon.fr/arch4edu/$arch
						Server = https://mirrors.tencent.com/arch4edu/$arch
						SigLevel = Never
					Endofpacman
	    fi
	}
	add_archlinuxcn_repo() {
	    if ! grep -q 'archlinuxcn' /etc/pacman.conf; then
	        cat >>/etc/pacman.conf <<-'Endofpacman'
						[archlinuxcn]
						Server = https://mirrors.bfsu.edu.cn/archlinuxcn/$arch
						Server = https://mirrors.tuna.tsinghua.edu.cn/archlinuxcn/$arch
						Server = https://repo.archlinuxcn.org/$arch
						SigLevel = Never
					Endofpacman
	    fi
	    printf "%s\n" "${GREEN}pacman ${YELLOW}-Syyu ${PURPLE}--noconfirm${RESET}"
	    pacman -Syyu --noconfirm
	}
	create_paru_script(){
		cat>paru<<-'EOFparu'
	#!/usr/bin/env bash
	if [ -e /usr/bin/paru ];then
		/usr/bin/paru $@
	else
		/opt/bin/paru $@
	fi
	EOFparu
	cat>yay<<-'EOFyay'
	#!/usr/bin/env bash
	if [ -e /usr/bin/yay ];then
	/usr/bin/yay $@
	else
	paru $@
	fi
	EOFyay
	install -Dm755 paru /usr/local/bin
	install -Dm755 yay /usr/local/bin
	}
	arch_linux_paru() {
		# pacman-key --populate archlinuxcn
		# archlinux-keyring archlinuxcn-keyring
	    for i in arch4edu-keyring paru; do
	        printf "%s\n" "${GREEN}pacman ${YELLOW}-Sy ${PURPLE}--noconfirm --needed ${BLUE}${i}${RESET}"
	        pacman -Sy --noconfirm --needed ${i}
	    done
		if [[ ! $(command -v paru) ]]; then
			get_arch_paru
		fi
	}
	get_arch_paru() {
		case $(uname -m) in
		x86_64) pacman -Sy --noconfirm --needed paru ;;
		armv7* | armv8l) 
		printf "%s\n" \
			"You should install ${GREEN}paru ${BLUE}manually${RESET}" \
			"${GREEN}git ${YELOW}clone --depth=1 ${BLUE}https://aur.archlinux.org/paru.git${RESET}" \
			"cd paru" \
			"makepkg -Si"
		;;
	    aarch64 | armv8* | armv9*)
		cd /tmp
		curl -Lo paru.tar.zst l.tmoe.me/paru-arm64
		tar -xvf paru.tar.zst
		mkdir -p /opt/bin
		install -Dm755 paru /opt/bin
		create_paru_script
		printf "%s\n" \
		"You should run ${GREEN}paru -Sy ${BLUE}paru-bin${RESET} to upgrade it."
		;;
		esac
		cd ~
	}
