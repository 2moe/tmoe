################
remove_debian_opt_repo() {
    non_debian_function
    rm -vf ${OPT_REPO_LIST} /etc/apt/trusted.gpg.d/bintray-public.key.asc
    apt update
}
##########
install_opt_vocal() {
    DEBIAN_INSTALLATION_MENU='00'
    DEPENDENCY_01='vocal'
    beta_features_quick_install
}
