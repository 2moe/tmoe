electron_apps_main_menu() {
    RETURN_TO_WHERE='electron_apps_main_menu'
    RETURN_TO_MENU='electron_apps_main_menu'
    DEPENDENCY_02=""
    if [ ! -e /opt/electron/electron ]; then
        printf "%s\n" "æœ¬electronä»“åº“ç”±2moeè¿›è¡Œç»´æŠ¤ï¼Œæ„Ÿè°¢å„ä¸ªé¡¹ç›®çš„åŸå¼€å‘è€…ã€‚"
        printf "%s\n" "æ‚¨å¯ä»¥é€šè¿‡æœ¬è„šæœ¬${PURPLE}æ‰‹åŠ¨æ£€æµ‹${RESET}æ›´æ–°ã€‚"
        printf "%s\n" "è¦æ±‚:${YELLOW}gnu libc${RESET}, æ”¯æŒ${BLUE}arch/manjaro,debian/ubuntu,fedora,opensuse,void(émuslç‰ˆ)ç­‰${RESET}"
        printf "%s\n" "æš‚ä¸æ”¯æŒalpine(musl libc)"
        press_enter_to_continue
    fi
    cd ${APPS_LNK_DIR}
    #16 50 7
    INSTALL_APP=$(whiptail --title "ELECTRON APPS" --menu \
        "electron:Build cross-platform desktop apps with JavaScript,HTML,and CSS" 0 0 0 \
        "1" "ğŸµ music:ä»¥é›…ä»¥å—,ä»¥é¾ ä¸åƒ­" \
        "2" "ğŸ“º videosè§†é¢‘:å…¨ç½‘å½±è§†æœç´¢,æ— æŸåˆ‡å‰²è§†é¢‘" \
        "3" "ğŸ“ notesç¬”è®°:è®°å½•çµæ„Ÿ,æ’°å†™æ–‡æ¡£,æ•´ç†ææ–™,å›é¡¾æ—¥è®°" \
        "4" "ğŸ virtual machineè™šæ‹Ÿæœº:win95,macos8" \
        "5" "ğŸ‘¾ developmentç¨‹åºå¼€å‘:ç¥ç»ç½‘ç»œ,æ·±åº¦å­¦ä¹ " \
        "6" "âš›ï¸ electron manager" \
        "0" "ğŸŒš Return to previous menu è¿”å›ä¸Šçº§èœå•" \
        3>&1 1>&2 2>&3)
    # "5" "snsç¤¾äº¤:electron-qq" \
    # 5) tmoe_electron_sns_app ;;
    ##############
    case "${INSTALL_APP}" in
    0 | "") software_center ;;
    1) tmoe_electron_music_app ;;
    2) tmoe_electron_video_app ;;
    3) tmoe_electron_note_app ;;
    4) tmoe_electron_virtual_machine_app ;;
